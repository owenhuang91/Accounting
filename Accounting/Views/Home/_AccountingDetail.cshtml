@model IEnumerable<Accounting.Models.ViewModels.AccountingDetailViewModel>
<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>@Html.DisplayNameFor(m => m.No)</th>
                <th>@Html.DisplayNameFor(m => m.Type)</th>
                <th>@Html.DisplayNameFor(m => m.Date)</th>
                <th>@Html.DisplayNameFor(m => m.Price)</th>
            </tr>

                @foreach (var accountingDetail in Model)
                {
                    <tr>
                        <td>@accountingDetail.No</td>
                        <td>@accountingDetail.Type</td>
                        <td>@accountingDetail.Date</td>
                        <td>@accountingDetail.Price</td>
                    </tr>
                }

        </table>
    </div>
</div>